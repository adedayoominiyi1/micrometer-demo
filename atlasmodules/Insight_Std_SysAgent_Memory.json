{
    "previewQuery": "",
    "description": "View system memory usage.",
    "categories": [
        "insight",
        "system",
        "memory"
    ],
    "owner": "brharrington@netflix.com",
    "script": true,
    "layout": {
        "numCols": 2
    },
    "rows": [
        {
            "cells": [
                {
                    "ylabel": "bytes",
                    "query": "name,(,mem.availReal,mem.totalReal,),:in,:max,(,name,),:by,$name,:legend",
                    "l": 0,
                    "tick_labels": "binary",
                    "title": "Real Memory Available"
                },
                {
                    "ylabel": "percent",
                    "u": 100,
                    "query": "name,mem.availReal,:eq,:max,name,mem.totalReal,:eq,:max,:div,100,:mul,percent available,:legend",
                    "l": 0,
                    "title": "Real Memory Free"
                }
            ]
        },
        {
            "cells": [
                {
                    "ylabel": "bytes",
                    "query": "name,(,mem.availSwap,mem.totalSwap,),:in,:max,(,name,),:by,$name,:legend",
                    "l": 0,
                    "tick_labels": "binary",
                    "title": "Swap Space Available"
                },
                {
                    "ylabel": "percent",
                    "u": 100,
                    "query": "name,mem.availSwap,:eq,:max,name,mem.totalSwap,:eq,:max,:div,100,:mul,percent available,:legend",
                    "l": 0,
                    "title": "Swap Space Free"
                }
            ]
        },
        {
            "cells": [
                {
                    "ylabel": "bytes",
                    "query": "name,(,mem.buffer,mem.cached,mem.shared,),:in,:max,(,name,),:by,$name,:legend,:stack",
                    "l": 0,
                    "tick_labels": "binary",
                    "title": "Memory Used by OS"
                },
                {
                    "query": "name,vmstat.paging,:eq,:sum,(,id,),:by,$id,:legend,:stack",
                    "ylabel": "pages/second",
                    "title": "Pages In/Out",
                    "l": 0
                }
            ]
        }
    ]
}