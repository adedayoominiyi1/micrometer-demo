{
    "previewQuery": "",
    "owner": "atlasdev@netflix.com",
    "layout": {
        "numCols": 2
    },
    "rows": [
        {
            "cells": [
                {
                    "title": "Incoming bits/sec",
                    "colspan": 4,
                    "query": "name,ifInOctets,:eq,:avg,8,:mul,ifInOctets,:legend"
                },
                {
                    "title": "Outgoing bits/sec",
                    "colspan": 4,
                    "query": "name,ifOutOctets,:eq,:avg,8,:mul,ifOutOctets,:legend"
                }
            ]
        },
        {
            "cells": [
                {
                    "title": "Outgoing and Incoming Connections",
                    "colspan": 4,
                    "query": "name,tcpActiveOpens,:eq,:sum,Outgoing%20Connections,:legend,name,tcpPassiveOpens,:eq,:sum,Incoming%20Connections,:legend"
                },
                {
                    "title": "Established Connections",
                    "colspan": 4,
                    "query": "name,tcpCurrEstab,:eq,:sum,Established%20Connections,:legend"
                }
            ]
        },
        {
            "cells": [
                {
                    "title": "Total TCP Errors",
                    "colspan": 4,
                    "query": "name,tcpRetransSegs,:eq,:sum,name,tcpInErrs,:eq,:sum,name,tcpOutRsts,:eq,:sum,name,tcpEstabResets,:eq,:sum,name,tcpAttemptFails,:eq,:sum",
                    "stack": 1
                },
                {
                    "title": "Percent Retransmitted Segments",
                    "colspan": 4,
                    "query": "name,tcpRetransSegs,:eq,:sum,name,tcpOutSegs,:eq,:sum,:div,100,:mul,Percentage%20of%20Retrans%20Segs,:legend"
                }
            ]
        }
    ]
}