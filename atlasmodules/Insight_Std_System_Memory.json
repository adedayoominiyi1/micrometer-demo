{
    "previewQuery": "",
    "description": "View system memory usage.",
    "categories": [
        "insight",
        "system",
        "memory"
    ],
    "owner": "brharrington@netflix.com",
    "script": true,
    "layout": {
        "numCols": 2
    },
    "rows": [
        {
            "cells": [
                {
                    "ylabel": "bytes",
                    "query": "name,(,memAvailReal,memTotalReal,),:in,:max,(,name,),:by,1024,:mul,$name,:legend",
                    "l": 0,
                    "tick_labels": "binary",
                    "title": "Real Memory Available"
                },
                {
                    "ylabel": "percent",
                    "u": 100,
                    "query": "name,memAvailReal,:eq,:max,name,memTotalReal,:eq,:max,:div,100,:mul,percent available,:legend",
                    "l": 0,
                    "title": "Real Memory Free"
                }
            ]
        },
        {
            "cells": [
                {
                    "ylabel": "bytes",
                    "query": "name,(,memAvailSwap,memTotalSwap,),:in,:max,(,name,),:by,1024,:mul,$name,:legend",
                    "l": 0,
                    "tick_labels": "binary",
                    "title": "Swap Space Available"
                },
                {
                    "ylabel": "percent",
                    "u": 100,
                    "query": "name,memAvailSwap,:eq,:max,name,memTotalSwap,:eq,:max,:div,100,:mul,percent available,:legend",
                    "l": 0,
                    "title": "Swap Space Free"
                }
            ]
        },
        {
            "cells": [
                {
                    "ylabel": "bytes",
                    "query": "name,(,memBuffer,memCached,memShared,),:in,:max,(,name,),:by,1024,:mul,$name,:legend,:stack",
                    "l": 0,
                    "tick_labels": "binary",
                    "title": "Memory Used by OS"
                },
                {
                    "query": "name,(,vmstat.page_in,vmstat.page_out,),:in,:sum,(,name,),:by,$name,:legend,:stack",
                    "ylabel": "pages/second",
                    "title": "Pages In/Out",
                    "l": 0
                }
            ]
        }
    ]
}